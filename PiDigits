 	{ 
 		public static void main(String[] args) 		
      { 
 			double pi = 3.1415926535897932384;
         System.out.println("The first 19 digits of Pi are "); 
         System.out.print(pi);
         System.out.println(" The next digit is 6 after " + pi);
 			 
 		} 
 	} 
